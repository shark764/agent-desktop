{
  "name": "agent-desktop",
  "version": "1.54.0",
  "description": "Agent Desktop App",
  "repository": {
    "type": "git",
    "url": "git@github.com:SerenovaLLC/agent-desktop.git"
  },
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "author": "Serenova",
  "license": "UNLICENSED",
  "scripts": {
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "npmCheckPhonevVersion": "node ./internals/scripts/phoneLibVersion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "webpack --mode production",
    "build:clean": "npm run test:clean && rimraf ./build",
    "format": "prettier-eslint --single-quote --trailing-comma es5 --write",
    "start": "webpack-dev-server --port 3003 --mode development",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint --ignore-pattern **/assets/*.js app",
    "lint:fix": "eslint app --fix",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:file": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "zendesk": "bash tasks/zendesk.sh",
    "precommit": "npm run npmCheckPhonevVersion && npm run lint:staged && npm run lint && npm run test"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint:fix",
      "git add -f"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "syntax-dynamic-import",
      "react-hot-loader/babel"
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "draft-js-export-html": "1.2.0",
    "draft-js-import-html": "1.2.1",
    "empty-module": "0.0.2",
    "express": "4.16.3",
    "font-awesome": "4.7.0",
    "google-libphonenumber": "3.1.8",
    "immutable": "v4.0.0-rc.2",
    "intl": "1.2.5",
    "ip": "1.1.5",
    "lodash": "4.17.10",
    "medium-draft": "https://github.com/SerenovaLLC/medium-draft",
    "minimist": "1.2.0",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "radium": "0.24.1",
    "raven-js": "3.26.4",
    "react": "16.4.1",
    "react-autocomplete": "1.8.1",
    "react-dom": "16.4.1",
    "react-dotdotdot": "1.2.3",
    "react-hot-loader": "4.3.4",
    "react-immutable-proptypes": "2.1.0",
    "react-infinite-scroller": "1.2.0",
    "react-input-autosize": "2.2.1",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "react-select": "1.2.1",
    "react-tabs": "2.2.2",
    "react-test-renderer": "16.4.1",
    "react-tether": "1.0.1",
    "react-textarea-autosize": "6.1.0",
    "react-timer-hoc": "2.3.0",
    "react-toggle": "4.0.2",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.7",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0",
    "redux-watch": "1.1.1",
    "reselect": "3.0.1",
    "sanitize.css": "6.0.0",
    "serenova-client-strings": "^0.1.10",
    "serenova-js-utils": "^4.0.0",
    "store": "2.0.12",
    "styled-components": "3.3.3",
    "validator": "10.4.0",
    "warning": "4.0.1",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "autoprefixer": "9.0.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cheerio": "0.22.0",
    "css-loader": "1.0.0",
    "cx-ui-components": "0.55.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-redux-saga": "0.9.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "0.8.0",
    "jest": "23.4.2",
    "jest-environment-jsdom": "23.4.0",
    "jest-environment-jsdom-global": "1.1.0",
    "json-loader": "0.5.7",
    "lint-staged": "7.2.0",
    "ngrok": "3.1.0",
    "null-loader": "0.1.1",
    "plop": "2.0.0",
    "postcss-loader": "2.1.6",
    "prettier-eslint-cli": "4.7.1",
    "react-beautiful-dnd": "^10.0.4",
    "react-color": "^2.17.0",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.16.3",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  }
}
