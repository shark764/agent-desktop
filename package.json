{
  "name": "agent-desktop",
  "version": "1.28.23",
  "description": "Agent Desktop App",
  "repository": {
    "type": "git",
    "url": "git@github.com:SerenovaLLC/agent-desktop.git"
  },
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "author": "Serenova",
  "license": "UNLICENSED",
  "scripts": {
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "webpack --mode production",
    "build:clean": "npm run test:clean && rimraf ./build",
    "format": "prettier-eslint --single-quote --trailing-comma es5 --write",
    "start": "webpack-dev-server --port 3000 --mode development",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:file": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "zendesk": "bash tasks/zendesk.sh",
    "precommit": "npm run lint:staged && npm run test"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add -f"
    ]
  },
  "jest": {
    "setupFiles": [
      "./internals/scripts/shim.js",
      "./internals/scripts/enzymeConfig.js"
    ],
    "setupTestFrameworkScriptFile": "./internals/scripts/setupTests.js",
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!cx-ui-components).+(js|jsx)$"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/**/messages.js",
      "!app/**/constants.js",
      "!app/**/actions.js",
      "!app/app.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "testRegex": "tests/.*\\.test\\.js$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "syntax-dynamic-import",
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "redux-saga",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "CxEngage": false
    },
    "rules": {
      "new-cap": [
        0,
        {
          "newIsCap": false
        }
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-return-await": 0,
      "no-await-in-loop": 0,
      "no-multi-assign": 0,
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-else-return": 0,
      "jsx-a11y/anchor-has-content": 0,
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/anchor-is-valid": 0,
      "prefer-destructuring": [
        1,
        {
          "object": true,
          "array": false
        }
      ],
      "react/jsx-indent": 0,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "react/no-typos": 1,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/no-autofocus": 0,
      "jsx-a11y/media-has-caption": 0,
      "jsx-a11y/accessible-emoji": 0,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 0,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/no-array-index-key": 1,
      "react/require-default-props": 0,
      "react/require-extension": 0,
      "redux-saga/no-yield-in-race": 2,
      "redux-saga/yield-effects": 2,
      "react/sort-comp": 0,
      "react/destructuring-assignment": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "cx-ui-components": "0.21.8",
    "draft-js-export-html": "1.2.0",
    "draft-js-import-html": "1.2.1",
    "empty-module": "0.0.2",
    "express": "4.16.3",
    "font-awesome": "4.7.0",
    "google-libphonenumber": "3.1.8",
    "immutable": "v4.0.0-rc.2",
    "intl": "1.2.5",
    "ip": "1.1.5",
    "lodash": "4.17.10",
    "medium-draft": "https://github.com/SerenovaLLC/medium-draft",
    "minimist": "1.2.0",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "radium": "0.24.1",
    "raven-js": "3.26.4",
    "react": "16.4.1",
    "react-autocomplete": "1.8.1",
    "react-dom": "16.4.1",
    "react-dotdotdot": "1.2.3",
    "react-hot-loader": "4.3.4",
    "react-immutable-proptypes": "2.1.0",
    "react-infinite-scroller": "1.2.0",
    "react-input-autosize": "2.2.1",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "react-select": "1.2.1",
    "react-tabs": "2.2.2",
    "react-test-renderer": "16.4.1",
    "react-tether": "1.0.1",
    "react-textarea-autosize": "6.1.0",
    "react-timer-hoc": "2.3.0",
    "react-toggle": "4.0.2",
    "react-tooltip": "3.6.1",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0",
    "redux-watch": "1.1.1",
    "reselect": "3.0.1",
    "sanitize.css": "6.0.0",
    "serenova-js-utils": "1.1.0",
    "store": "2.0.12",
    "styled-components": "3.3.3",
    "url-polyfill": "1.0.13",
    "validator": "10.4.0",
    "warning": "4.0.1",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "autoprefixer": "9.0.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cheerio": "0.22.0",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-redux-saga": "0.9.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "0.8.0",
    "jest": "23.4.2",
    "jest-environment-jsdom": "23.4.0",
    "jest-environment-jsdom-global": "1.1.0",
    "json-loader": "0.5.7",
    "lint-staged": "7.2.0",
    "ngrok": "3.0.1",
    "null-loader": "0.1.1",
    "plop": "2.0.0",
    "postcss-loader": "2.1.6",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.16.3",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  }
}
